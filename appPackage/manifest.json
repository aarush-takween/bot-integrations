{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Katonic AI",
        "websiteUrl": "https://katonic.ai/",
        "privacyUrl": "https://katonic.ai/privacy-policy.html",
        "termsOfUseUrl": "https://katonic.ai/terms-and-conditions.html"
    },
    "icons": {
        "color": "katonic-color.png",
        "outline": "katonic-outline.png"
    },
    "name": {
        "short": "Katonic AI Bot",
        "full": "Katonic GenAI ChatBot"
    },
    "description": {
        "short": "Katonic AI: MLOps platform for enterprise AI",
        "full": "Katonic AI is an MLOps platform that helps businesses realize the value of enterprise AI faster. The platform combines the creative scientific process of data scientists with the professional software engineering process to build and deploy machine learning models into production safely, quickly, and sustainably."
    },
    "accentColor": "#5C31FF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}